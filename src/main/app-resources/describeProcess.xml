<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessDescriptions xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ows="http://www.opengis.net/ows/1.1" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsDescribeProcess_response.xsd" xml:lang="en-US" service="WPS" version="1.0.0">
  <ProcessDescription statusSupported="true" storeSupported="true" wps:processVersion="1.0.0">
    <ows:Identifier>stemp-s2</ows:Identifier>
    <ows:Title>STEMP-S2</ows:Title>
    <ows:Abstract>ERUPTIVE HOT SPOT detection obtained by multi-spectral optical sensors (Sentinel-2) generated in GeoTIFF format</ows:Abstract>
    <DataInputs>
      <Input minOccurs="1" maxOccurs="1">
        <ows:Identifier>startdate</ows:Identifier>
        <ows:Title>Start date</ows:Title>
        <ows:Abstract>Format YYYY-MM-DDThh:mm:ssZ or YYYY-MM-DD</ows:Abstract>
        <LiteralData>
          <ows:DataType ows:reference="xs:string">string</ows:DataType>
          <ows:AnyValue/>
          <DefaultValue/>
        </LiteralData>
      </Input>
      <Input minOccurs="1" maxOccurs="1">
        <ows:Identifier>enddate</ows:Identifier>
        <ows:Title>End date</ows:Title>
        <ows:Abstract>Format YYYY-MM-DDThh:mm:ssZ or YYYY-MM-DD</ows:Abstract>
        <LiteralData>
          <ows:DataType ows:reference="xs:string">string</ows:DataType>
          <ows:AnyValue/>
          <DefaultValue/>
        </LiteralData>
      </Input>
      <Input minOccurs="1" maxOccurs="1">
        <ows:Identifier>volcano</ows:Identifier>
        <ows:Title>Volcano name</ows:Title>
        <ows:Abstract>Volcano where to perform the processing</ows:Abstract>
        <LiteralData>
          <ows:DataType ows:reference="xs:string">string</ows:DataType>
          <ows:AllowedValues>
            <ows:Value>Arenal</ows:Value>
            <ows:Value>Chachimbiro</ows:Value>
            <ows:Value>Colima</ows:Value>
            <ows:Value>Cotacachi</ows:Value>
            <ows:Value>Cotopaxi</ows:Value>
            <ows:Value>Dukono</ows:Value>
            <ows:Value>Etna</ows:Value>
            <ows:Value>Ibu</ows:Value>
            <ows:Value>La Cumbre Fernandina</ows:Value>
            <ows:Value>Merapi</ows:Value>
            <ows:Value>Piton de la Fournaise</ows:Value>
            <ows:Value>Poas</ows:Value>
            <ows:Value>Puyehue-Cordon Caulle</ows:Value>
            <ows:Value>Reventador</ows:Value>
            <ows:Value>Sangai</ows:Value>
            <ows:Value>Santiaguito</ows:Value>
            <ows:Value>Sierra Negra</ows:Value>
            <ows:Value>Sinabung</ows:Value>
            <ows:Value>Stromboli</ows:Value>
            <ows:Value>Tungurahua</ows:Value>
            <ows:Value>Turrialba</ows:Value>
            <ows:Value>Vesuvio Campi Flegrei</ows:Value>
          </ows:AllowedValues>
          <DefaultValue>Etna</DefaultValue>
        </LiteralData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>result_distribution</ows:Identifier>
        <ows:Title>Result Files Distribution Package</ows:Title>
        <ows:Abstract>This process returns a file with the list of result products.
The default is Metalink document that is an extensible metadata file format that describes one or more computer files available for download. It specifies files appropriate for the user's language and operating system; facilitates file verification and recovery from data corruption.</ows:Abstract>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>application/xml</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/xml</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
      <Output>
        <ows:Identifier>result_osd</ows:Identifier>
        <ows:Title>OpenSearch Description to the Results</ows:Title>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>application/xml</MimeType>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/xml</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>
