<?xml version="1.0" encoding="us-ascii"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="application">
  <jobTemplates>
    <jobTemplate id="query">
      <streamingExecutable>/application/query/run.sh</streamingExecutable>
      <defaultParameters>
        <parameter id="startdate">${startdate}</parameter>
        <parameter id="enddate">${enddate}</parameter>
        <parameter id="volcano">${volcano}</parameter>
      </defaultParameters>
      <defaultJobconf>
        <property id="ciop.job.max.tasks">1</property>
      </defaultJobconf>
    </jobTemplate>
    <jobTemplate id="stemp">
      <streamingExecutable>/application/stemp/run.sh</streamingExecutable>
      <defaultJobconf>
        <property id="mapred.task.timeout">3600000</property>
      </defaultJobconf>
    </jobTemplate>
  </jobTemplates>
  <workflow id="dcs-stemp-s2">
    <workflowVersion>1.0</workflowVersion>
    <node id="node-query">
      <job id="query"/>
      <sources>
        <source id="dummy" refid="string:list">dummy</source>
      </sources>
    </node>
    <node id="node-stemp">
      <job id="stemp"/>
      <sources>
        <source refid="wf:node">node-query</source>
      </sources>
    </node>
  </workflow>
</application>
